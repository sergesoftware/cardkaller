
import java.util.Stack;
import net.rim.device.api.util.Persistable;
class MyStackWrapper implements Persistable
{
    private static Stack myStack;
    
    MyStackWrapper()
    {
        myStack = new Stack();
    }
    public void setStack(Stack stack)
    {
        myStack = stack;
    }
    public Stack getStack()
    {
        return myStack;
    }
}
